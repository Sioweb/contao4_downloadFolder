{
    "name": "sioweb/downloadfolder",
    "type": "contao-bundle",
    "description": "Download whole folder as ZIP for Contao 4",
    "license": "MIT",
    "authors": [
        {
            "name": "Sascha Weidner",
            "email": "support@sioweb.de"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "contao/core-bundle": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Sioweb\\DownloadFolderBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Sioweb\\DownloadFolderBundle\\ContaoManager\\Plugin"
    }
}
